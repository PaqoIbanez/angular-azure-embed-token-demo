{
  "routes": [
    {
      "routes": [
        {
          "route": "/assets/*",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/*.css",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/*.js",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/*.ttf",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/*.woff",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/*.woff2",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/favicon.ico",
          "allowedRoles": [
            "anonymous"
          ]
        },
        {
          "route": "/*",
          "rewrite": "/index.html"
        }
      ]
    }
  ]
}